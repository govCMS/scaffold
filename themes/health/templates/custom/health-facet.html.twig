<div class="health-facet">
  <h2 class="au-display-sm">{{title}}</h2>
  <div id="{{ id }}" class="health-facet__list">
    {% block items %}
      {%- if items -%}
        {%- for item in items -%}
          <div class="au-control-input au-control-input--compact au-control-input--block">
            <input type="checkbox" id="{{ item.id }}"
              name="{{ item.id }}"
              value="{{ item.value }}"
              class="au-control-input__input"/>
            <label class="au-control-input__text" for="{{ item.id }}">{{ item.value }}</label>
          </div>
        {%- endfor -%}
      {%- else -%}
        {{- empty -}}
      {%- endif -%}
    {% endblock %}
  </div>
</div>
